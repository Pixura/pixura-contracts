{
  "name": "pixura-contracts-solc-v6",
  "version": "0.0.1",
  "source-dir": "contracts/src",
  "artifacts-dir": "contracts/build/",
  "extra-abis": "contracts/abis",
  "modules": [
    "SuperRareAuctionHouse",
    "SuperRareRoyaltyRegistry",
    "SuperRareTokenCreatorRegistry",
    "IERC721CreatorRoyalty",
    "IERC721TokenCreator",
    "Marketplace.IMarketplaceSettings",
    "Marketplace.MarketplaceSettings",
    "MaybeSendValue",
    "SendValueOrEscrow",
    "SuperRareMarketAuctionV2",
    "TestAssertFailOnPay",
    "TestRequireFailOnPay",
    "TestExpensiveWallet",
    "TestRevertOnPay",
    "ISupeRare",
    "SuperRareLegacy"
  ],
  "dependencies": [
    "openzeppelin-solidity-solc6",
    "openzeppelin-solidity-pixura"
  ],
  "solc-optimizer": {
    "enabled": false,
    "runs": 200
  },
  "solc-version": "0.6.12",
  "purescript-generator": {
    "output-path": "./purs-contracts/src",
    "module-prefix": "Contracts"
  }
}
